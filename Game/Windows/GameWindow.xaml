<Window x:Class="Game.Windows.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game.Windows"
        xmlns:objects="clr-namespace:Game.ObjectsBase"
        mc:Ignorable="d"
        Title="GameWindow" Height="450" Width="800">
    <DockPanel>
        <StackPanel x:Name="plantChoosePanel" Width="75">

        </StackPanel>
        <Viewbox>
            <Canvas x:Name="gameCanvas" Background="DarkOliveGreen" Width="800" Height="450"/>
        </Viewbox>
    </DockPanel>
</Window>
